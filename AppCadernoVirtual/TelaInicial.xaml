<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppCadernoVirtual.TelaInicial">
    <ContentPage.Content>
        <StackLayout>

            <Label Text="Matérias" FontSize="Large" FontAttributes="Bold" HorizontalTextAlignment="Center"/>
            <ImageButton Aspect="Fill"  x:Name="PortuguesPrimeiro" Clicked="PortuguesPrimeiro_Clicked"   Grid.Column="0" Grid.Row="1"/>
            <ImageButton Aspect="Fill" x:Name="InglesPrimeiro" Clicked="InglesPrimeiro_Clicked"   Grid.Column="1" Grid.Row="2"/>
            <ImageButton Aspect="Fill" x:Name="MatematicaPrimeiro" Clicked="MatematicaPrimeiro_Clicked"   Grid.Column="0" Grid.Row="3"/>
            <ImageButton Aspect="Fill" x:Name="QuimicaPrimeiro" Clicked="QuimicaPrimeiro_Clicked"   Grid.Column="1" Grid.Row="1"/>
            <ImageButton Aspect="Fill" x:Name="FisicaPrimeiro" Clicked="FisicaPrimeiro_Clicked"   Grid.Column="1" Grid.Row="2"/>
            <ImageButton Aspect="Fill" x:Name="BiologiaPrimeiro" Clicked="BiologiaPrimeiro_Clicked"   Grid.Column="1" Grid.Row="3"/>
            <ImageButton Aspect="Fill" x:Name="GeografiaPrimeiro" Clicked="GeografiaPrimeiro_Clicked"   Grid.Column="2" Grid.Row="1"/>
            <ImageButton Aspect="Fill" x:Name="HistoriaPrimeiro" Clicked="HistoriaPrimeiro_Clicked"   Grid.Column="2" Grid.Row="2"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>